    "morgan": "^1.9.0",
    "node-notifier": "^5.2.1",
    "npm-run-all": "^4.1.3",
    "offline-plugin": "^5.0.5",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.5",
    "react-hot-loader": "^4.3.3",
